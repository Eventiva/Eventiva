{
	"name": "Eventiva Development Environment",
	"dockerComposeFile": [
		"../compose.yaml",
		"../overrides/compose.mariadb.yaml",
		"../overrides/compose.redis.yaml",
		"../overrides/compose.https.yaml",
		"./compose.eventiva.yaml",
		"./compose.bot.discord.yaml"
	],
	"service": "backend",
	"workspaceFolder": "/home/frappe/frappe-bench/",
	"containerEnv": {
		"ERPNEXT_VERSION": "v15.2.0",
		"DB_HOST": "db",
		"DB_PORT": "3306",
		"REDIS_CACHE": "redis-cache:6379",
		"REDIS_QUEUE": "redis-queue:6379",
		"SOCKETIO_PORT": "redis-socketio:6379",
		"LETSENCRYPT_EMAIL": "system@eventiva.co.uk",
		"SITE_ADMIN_PASS": "admin",
		"DB_PASSWORD": "databasePassword",
		"SITE_NAME": "site1.localhost"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.black-formatter",
				"ms-vscode-remote.remote-containers",
				"benjaminbenais.copilot-theme",
				"github.vscode-github-actions",
				"GitHub.copilot",
				"GitHub.copilot-chat",
				"GitHub.copilot-labs",
				"Massi.javascript-docstrings",
				"crystal-spider.jsdoc-generator",
				"bierner.jsdoc-markdown-highlighting",
				"PKief.material-icon-theme",
				"PKief.material-product-icons",
				"ms-edgedevtools.vscode-edge-devtools",
				"CodeStream.codestream",
				"christian-kohler.npm-intellisense",
				"codeandstuff.package-json-upgrade",
				"psioniq.psi-header",
				"ms-python.vscode-pylance",
				"ms-python.python",
				"WallabyJs.quokka-vscode",
				"meganrogge.template-string-converter",
				"rangav.vscode-thunder-client",
				"WallabyJs.wallaby-extension-pack",
				"samverschueren.linter-xo",
				"yzhang.markdown-all-in-one"
			]
		},
		"settings": {
			"terminal.integrated.profiles.linux": {
				"frappe bash": {
					"path": "/bin/bash"
				}
			},
			"terminal.integrated.defaultProfile.linux": "frappe bash",
			"python.terminal.activateEnvInCurrentTerminal": true,
			"debug.node.autoAttach": "disabled"
		}
	},
	"forwardPorts": [
		5432,
		8000,
		9000,
		6787,
		4000
	]
}