              },
              {
                "type": "titleMatches",
                "condition": "/^maintenance(\\(.*\\))?!?:/i"
              },
              {
                "type": "descriptionMatches",
                "condition": "/^type:(,| |Refactoring|Optimisation|Revert|Deprecated|Removal)*style/im"
              }
            ]
          },
          "refactor": {
            "requires": 2,
            "condition": [
              {
                "type": "titleMatches",
                "condition": "/^ref(\\(.*\\))?!?:/i"
