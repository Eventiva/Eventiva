{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.97.1",
  "pnpmVersion": "7.10.0",
  "nodeSupportedVersionRange": ">=14.15.0 <15.0.0 || >=16.13.0 <17.0.0 || >=18.15.0 <19.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 3,
  "projectFolderMaxDepth": 3,
  "approvedPackagesPolicy": {
    "reviewCategories": [
      "production",
      "tools",
      "prototypes"
    ],
    "ignoredNpmScopes": [
      "@types"
    ]
  },
  "gitPolicy": {
    "allowedEmailRegExps": [
      "[^@]+@users\\.noreply\\.github\\.com",
      ".*@eventiva\\.co\\.uk",
      ".*@eventiva\\.com",
      ".*@resnovas\\.com"
    ],
    "sampleEmail": "mrexample@users.noreply.github.com",
    "versionBumpCommitMessage": "chore: Bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "docs: Update changelogs [skip ci]"
  },
  "repository": {
    "url": "https://github.com/Resnovas/Eventiva",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [
      "common/scripts/pre-rush-install.js"
    ],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "@eventiva/website",
      "projectFolder": "projects/website/",
      "reviewCategory": "prototypes",
      "versionPolicyName": "individualVersion",
      "shouldPublish": false
    }
  ]
}
